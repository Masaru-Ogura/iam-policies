{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "*",
      "Resource": "*"
    },
    {
      "Effect": "Deny",
      "Action": [
        "ec2:*ReservedInstance*",
        "rds:*ReservedDBInstance*",
        "redshift:*ReservedNode*",
        "elasticache:*ReservedCacheNodes*"
      ],
      "Resource": "*"
    }
  ]
}
