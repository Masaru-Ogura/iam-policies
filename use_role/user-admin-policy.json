{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "*",
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "support:*",
                "ec2:*ReservedInstance*",
                "rds:*ReservedDBInstance*",
                "redshift:*ReservedNode*",
                "elasticache:*ReservedCacheNodes*",
                "aws-portal:ViewBilling",
                "iam:DeleteAccountAlias",
                "iam:DeleteGroup",
                "iam:DeleteGroupPolicy",
                "iam:UpdateGroup",
                "iam:PutUserPolicy",
                "iam:PutGroupPolicy",
                "organizations:*"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:DeleteUser",
                "iam:DeleteUserPolicy",
                "iam:UpdateUser"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:user/administrator"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:DeleteUser",
                "iam:UpdateUser",
                "iam:DeleteUserPolicy",
                "iam:GetUserPolicy",
                "iam:PutUserPolicy",
                "iam:CreateLoginProfile",
                "iam:UpdateLoginProfile",
                "iam:DeleteLoginProfile",
                "iam:CreateAccessKey",
                "iam:UpdateAccessKey",
                "iam:DeleteAccessKey"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:user/serverworks",
                "arn:aws:iam::123456789123:user/cloudautomator"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:*"
            ],
            "Resource": [
                "arn:aws:iam::123456789123user/*",
                "arn:aws:iam::123456789123:group/*"
            ],
            "Condition": {
                "ArnLike": {
                    "iam:PolicyArn": [
                        "arn:aws:iam::aws:policy/job-function/SupportUser",
                        "arn:aws:iam::aws:policy/AWSSupportAccess",
                        "arn:aws:iam::aws:policy/AdministratorAccess",
                        "arn:aws:iam::aws:policy/IAMFullAccess",
                        "arn:aws:iam::123456789123:policy/*"
                    ]
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:DeleteRole",
                "iam:AttachRolePolicy",
                "iam:DeleteRolePolicy",
                "iam:DetachRolePolicy",
                "iam:PutRolePolicy",
                "iam:UpdateAssumeRolePolicy"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:role/ServerworksControlRole*"
            ]
        }
    ]
}
