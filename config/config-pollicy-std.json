{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": [
                "support:*",
                "aws-portal:ViewBilling",
                "trustedadvisor:*NotificationPreferences"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "ec2:*ReservedInstance*",
                "rds:*ReservedDBInstance*",
                "redshift:*ReservedNode*",
                "elasticache:*ReservedCacheNodes*"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:DeleteUser"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:user/[user-admin]"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:ChangePassword",
                "iam:DeleteUser",
                "iam:DeleteUserPolicy",
                "iam:GetUserPolicy",
                "iam:UpdateUser",
                "iam:CreateLoginProfile"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:user/[serverworks-user]",
                "arn:aws:iam::123456789123:user/[ca-user]"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:CreatePolicyVersion",
                "iam:DeletePolicy",
                "iam:AttachPolicy",
                "iam:DeletePolicy",
                "iam:DetachRolePolicy",
                "iam:DetachUserPolicy",
                "iam:DeletePolicyVersion"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:policy/ServerworksControlPolicy*"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:DeleteRole",
                "iam:AttachRolePolicy",
                "iam:DeleteRolePolicy",
                "iam:DetachRolePolicy",
                "iam:PutRolePolicy",
                "iam:UpdateAssumeRolePolicy"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:role/ServerworksControlRole*"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:A*",
                "iam:C*",
                "iam:Deactive*",
                "iam:Delete*",
                "iam:E*",
                "iam:G*",
                "iam:P*",
                "iam:R*",
                "iam:S*",
                "iam:U*"
            ],
            "Resource": [
                "arn:aws:iam::123456789123:user/*",
                "arn:aws:iam::123456789123:group/*",
                "arn:aws:iam::123456789123:role/*"
            ],
            "Condition": {
                "ArnLike": {
                    "iam:PolicyArn": [
                        "arn:aws:iam::123456789123:role/ServerworksControlRole*",
                        "arn:aws:iam::123456789123:policy/ServerworksControlPolicy*"
                    ]
                }
            }
        }
    ]
}
